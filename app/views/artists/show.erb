<h1> <%= @artist.name %>'s Artist Page </h1>
<h4> Songs: </h4>
<ul>
<%  @artist.songs.each do |song|  %>
<li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
<%end%>
</ul>
<h4> Genres: </h4>
<ul>
<%  @artist.genres.each do |genre|  %>
<li><a href="/genres/<%= genre.slug %>" id="<%=genre.name%>"><%= genre.name.capitalize %></a></li>
<% end %>
</ul>
